# Color variable names based off 16 color terminal defaults
black='\[\e[0;30m\]'
red='\[\e[0;31m\]'
green='\[\e[0;32m\]'
yellow='\[\e[0;33m\]'
blue='\[\e[0;34m\]'
magenta='\[\e[0;35m\]'
cyan='\[\e[0;36m\]'
lightgray='\[\e[0;90m\]'
white='\[\e[0;97m\]'

export PS1="$green\u$white@$green\h $white\W:"
